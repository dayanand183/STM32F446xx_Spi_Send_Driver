#include "stm32f446xx.h"

//	SPI_Handle_t SPI1;

	/*
	 * PB12 --> SPI2_NSS
	 * PB13 --> SPI2_CLK
	 * PB14 --> SPI2_MISO
	 * PB15 --> SPI2_MOSI
	 * ALT Func Mode 5
	 */

void SPI_GPIOInits(void)
{
	GPIO_Handle_t SPIPins;

	SPIPins.pGPIOx = GPIOB;
	SPIPins.GPIO_Config.Gpio_Pinmode = GPIO_ALTFUN_MODE;
	SPIPins.GPIO_Config.Gpio_Altfun = 5;
	SPIPins.GPIO_Config.Gpio_OPType = GPIO_OP_TYPE_PP;
	SPIPins.GPIO_Config.Gpio_PUPD = GPIO_NO_PU_PD;
	SPIPins.GPIO_Config.Gpio_Speed = GPIO_FAST_SPEED;
	SPIPins.GPIO_Config.Gpio_PinNumber = GPIO_PIN_NO_12 | GPIO_PIN_NO_13 | GPIO_PIN_NO_14 | GPIO_PIN_NO_15;

	GPIO_InIt(&SPIPins);
}


int main(void)
{


	while(1)
	{

	}

	return 0;
}
